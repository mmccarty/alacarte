
<%form_tag({:action => 'update_module', :id => @mod, :type =>@mod.class} , {:multipart => true})  do %>
 <fieldset> 
 <h3>Edit <%h@mod.module_title%> on <%=h@page.header_title%></h3>
    <%= show_form(@mod.class.to_s)%>
<div id="submit_button">
	<%= submit_tag 'SAVE', :class => "submit" %>  
	<p><%= link_to 'Cancel', :action => 'edit', :id => @page.id %></p>
</div>
</fieldset> 
<% end %>


 <%=render :partial => 'side_bar'%>