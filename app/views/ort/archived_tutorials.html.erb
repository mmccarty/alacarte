<% content_for :search_results do %>
  <h2>Browse Tutorials</h2>

  <dl>
    <% tutorials.each do |tutorial| %>
      <%= render :partial => "shared/list_page",
                 :locals => { :page => tutorial,
                              :controller => "ort" } %>
    <% end %>
  </dl>
<% end %>

<%= render :partial => "shared/published_page_results",
           :locals => { :header_title => @local.tutorial_page_title } %>
