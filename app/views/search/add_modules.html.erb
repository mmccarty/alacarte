<%search_results_message%>
<fieldset>
  <h3 class='title'>
    <%=setTitleAction%>
  </h3>
  <h4>Add Modules</h4>
  <% if flash[:error] -%>
  <div id ="error_wrapper">    <div id="list_error" ><%= flash[:error] %></div></div>
  <% end -%>
  <% if flash[:notice] -%>
  <div id="list_error" ><%= flash[:notice] %></div>
  <% end -%>
  <p> <b><%= set_tab_unit_message %> </b>

    <span class = 'tip'>
      <%= setEditLink %>
    </span>
  </p>
  <%=render :partial => "shared/search_form"%>

  <%unless @search_results.size <1%>
	   <div id="product-list">
	     <%= form_tag :controller => set_controller, :action => set_action, :id => set_id  do %>
  <div id="table">
    <%=render :partial => "shared/add_modules_list"%>
  </div>
  <%= submit_tag 'Add to ' + set_tab_or_unit, :class=> "submit" %>
  <%end%>
</div>
<%else%>
<p>
  <b>You currently have no Modules. Create a new module or copy a global module to get started.</b>
</p>

<%end%>

<%= setSideBar %>
</fieldset>
