<dl>
  <% pages.each do |page| %>
    <% if defined?(subject_code) and defined?(campus_hash) %>
      <%= render :partial => "shared/list_page",
                 :locals => { :page => page,
                              :controller => controller,
                              :subject_code => subject_code,
                              :campus_hash => campus_hash } %>
    <% else %>
      <%= render :partial => "shared/list_page",
                 :locals => { :page => page,
                              :controller => controller } %>
    <% end %>
  <% end %>
</dl>
