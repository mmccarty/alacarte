%fieldset
  %h3.title= render :partial => 'title_actions'
  %h4 Title &amp; Metadata
  - if flash[:error]
    #error_wrapper
      #list_error= flash[:error]
  #share
    = form_tag({:action => 'update', :id => @tutorial.id}, {:multipart => true}) do
      = render :partial => 'form'
      = link_to 'Cancel', tutorials_path, class: 'btn btn-cancel'
= render :partial => 'side_bar'
