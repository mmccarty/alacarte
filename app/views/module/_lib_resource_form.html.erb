<label for="librarian_name" >Librarian Name
  <span class = 'more_help' onMouseover="fixedtooltip(LIB_NAME, this, event, '');" onMouseout="delayhidetip();"><%= image_tag("icons/small_help.png", :alt => 'Tip', :border => 0)%></span> </label>
<%= text_field 'mod', 'librarian_name',"size" => 54   %>
